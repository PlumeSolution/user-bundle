{
    "name": "plume-solution/user-bundle",
    "description": "A new bundle for easy user management",
    "version": "1.1.1",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "GothShoot",
            "email": "contact@plumesolution.fr"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4 | ^8.0",
        "symfony/security-bundle": "^4.4 | ^5.0",
        "doctrine/doctrine-bundle": "^2.0",
        "symfony/framework-bundle": ">=4.4",
        "symfony/form": "^4.4 | ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0.14",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "suggest": {
        "symfony/twig-bundle": "For view support of user system"
    },
    "autoload": {
        "psr-4": {
            "PlumeSolution\\UserBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PlumeSolution\\UserBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests": [
            "phpunit",
            "phpcs"
        ],
        "cs": [
            "phpcs"
        ]
    }
}
