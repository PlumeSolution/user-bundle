{
    "name": "plume-solution/user-bundle",
    "description": "A new bundle for easy user management",
    "version": "1.0.0-dev",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "GothShoot",
            "email": "contact@plumesolution.fr"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "symfony/security-bundle": "^5.0",
        "symfony/orm-pack": "^1.0",
        "doctrine/doctrine-bundle": "^2.0",
        "symfony/framework-bundle": "^5.0",
        "symfony/form": "^5.0",
        "symfony/twig-bundle": "^5.0"
    },
    "suggest": {
        "php": ">=7.4 for fixing deprecated package and better performance"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "PSUserBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PSUserBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests": [
            "phpunit",
            "phpcs"
        ]
    }
}
